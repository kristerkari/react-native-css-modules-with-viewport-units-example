{
  "name": "CSSViewportUnitsDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint . && stylelint '**/*.@(css|scss)'",
    "web": "NODE_ENV=development webpack-dev-server --mode development",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "dependencies": {
    "es6-shim": "^0.35.3",
    "react": "16.4.1",
    "react-art": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-native": "0.56.0",
    "react-native-orientation-change-provider": "^0.11.2",
    "react-native-web": "^0.8.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-native-classname-to-dynamic-style": "^0.20.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "5.0.2",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.1",
    "eslint-plugin-css-modules": "^2.7.5",
    "jest": "23.1.0",
    "react-hot-loader": "3.1.3",
    "react-native-css-transformer": "^1.2.0",
    "react-test-renderer": "16.3.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-react-native-css-modules": "^1.4.0",
    "stylelint-react-native": "^1.1.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "preset": "react-native"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "css-modules"
    ],
    "extends": [
      "plugin:css-modules/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "unit-whitelist": [
        "vh",
        "vw",
        "vmin",
        "vmax",
        "px",
        "rem",
        "%"
      ],
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
